{
  "name": "@zerocopter/nav-core",
  "version": "0.3.0",
  "description": "Shared navigation and auth logic for Zerocopter's Next.js app and Framer marketing site",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "keycloak-js": ">=22.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "keycloak-js": "^26.2.1",
    "react": "^19.2.3",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "zerocopter",
    "navigation",
    "keycloak",
    "auth",
    "framer-bridge"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/TimnZC/zc-framer-bridge.git"
  }
}
